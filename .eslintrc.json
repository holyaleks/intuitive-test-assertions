{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "indent": "off",
        "semi": "off",
        "space-before-function-paren": "off",
        "no-extend-native": "off",
        "no-useless-constructor": "off",
        "no-new-wrappers": "off",
        "no-new": "off",
        "no-use-before-define": "warn",
        "no-unused-vars": "off"
    }
}
